{
  "name": "spotify-wrapper-player",
  "version": "1.0.0",
  "description": "A player using the spotify-wrapper.",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "npm run build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "prepush": "npm run lint",
    "test": "./node_modules/.bin/mocha --require @babel/register tests/**/*.spec.js",
    "test:tdd": "./node_modules/.bin/mocha --require @babel/register tests/**/*.spec.js --watch",
    "test:coverage": "nyc npm test",
    "start": "./node_modules/.bin/webpack-dev-server --open"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucascco/spotify-wrapper-player.git"
  },
  "keywords": [
    "js",
    "tdd",
    "library"
  ],
  "author": "Lucas CorrÃªa <lucasccorrea@gmail.com> (https://github.com/lucascco)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lucascco/spotify-wrapper-player/issues"
  },
  "homepage": "https://github.com/lucascco/spotify-wrapper-player#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "coffeescript": "^2.3.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.1.0",
    "jsdom": "12.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "spotify-wrapper": "^2.1.0"
  }
}
